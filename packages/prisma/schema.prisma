generator client {
  provider = "prisma-client-js"
  // bundle multiple platforms so it runs on Windows locally and Linux on host
  binaryTargets = ["native", "debian-openssl-3.0.x", "debian-openssl-1.1.x", "linux-musl"]
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:admin@localhost:3306/rage"
}


model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  age     Int
  gender  String
  password String
  money Float
  admin Int
  helper Int
  level Int @default(1)
}